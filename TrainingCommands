

sort rockyou.txt | uniq -u > rockyou.unique.txt
du -sb rockyou.* 
sort -R rockyou.unique.txt > rockyou.random.txt
head -n 250000 rockyou.random.txt > train.txt
sort train.txt test.txt | uniq -d | wc -l 


cat pws-* | sort | uniq -u > output-1.txt
wc -l output-1.txt 
31441 output-1.txt  
sort output-1.txt train.txt | uniq -d | wc -l 
sort output-1.txt test.txt | uniq -d | wc -l 

[RUNNING]
GPU 0: BASIC 8-12
python scripts/preprocess.py --input_txt inputC.txt --output_h5 basic8-12.h5 --output_json basic8-12.json
th train.lua -checkpoint_name cv_Basic812/Checkpoint -gpu 0 -input_h5 basic8-12.h5 -input_json basic8-12.json -model_type lstm -num_layers 3 -rnn_size 256 -max_epochs 15


iconv -f utf-8 -t utf-8 -c input.txt > inputC.txt
iconv -f utf-8 -t utf-8 -c inputR.txt > inputC.txt


python scripts/preprocess.py --input_txt inputC.txt --output_h5 basic8-12C.h5 --output_json basic8-12C.json
th train.lua -checkpoint_name cv_Basic8-12C/Checkpoint -gpu 1 -input_h5 basic8-12C.h5 -input_json basic8-12C.json -model_type lstm -num_layers 3 -rnn_size 256 -max_epochs 15

